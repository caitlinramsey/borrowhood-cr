<div class='navbar bg-base-100'>
  <div class='navbar-start'>
    <div class='dropdown'>
      {{#if isAuthenticated}}
      <label tabindex='0' class='btn btn-ghost btn-circle'>
        <svg xmlns='http://www.w3.org/2000/svg' class='h-45 w-45 burger' fill='none' viewBox='0 0 30 30'
          stroke='currentColor'>
          <path stroke-linecap='round' stroke-linejoin='round' stroke-width='2' d='M4 6h16M4 12h16M4 18h7' />
        </svg>
      </label>
      <ul tabindex='0' class='menu menu-sm dropdown-content mt-3 z-[1] p-2 shadow rounded-box w-52'>
        <li><a href="/feed"><strong>Home</strong></a></li>
        <li><a href="/dashboard"><strong>Dashboard</strong></a></li>
        <li><a href="/profile"><strong>Profile</strong></a></li>
      </ul>
      {{ else}}
      {{/if}}
    </div>
    <div class="about-us-nav">
      <a href="/aboutus"><strong>About Us</strong></a>
    </div>
    <div class="github">
      <a href='https://github.com/caitlinramsey/borrowhood-cr'  target='_blank'><i class='fa fa-github' style='font-size:40px; color:black;'></i></a>
    </div>
  </div>
  
  <div class='navbar-center'>
    <a href='/feed'>
<img src='images/borrowhoodlogo.png' class='logo' alt='logo'></a>
  </div>
  <div class='navbar-end'>
    {{#if isAuthenticated}}
      <a role='button' href='/' id="logout" class="logout-icon">{{currentUser.firstName}} <i class="fa fa-sign-out"></i></a>
    {{ else}}
    <button class='btn btn-accent navbar-btn'>
      <a role='button' class='nav-login' href='/login'>Log In</a>
    </button>
    <button class='btn btn-accent btn-circle navbar-btn'>
      <a role='button' class='nav-register' href='/register'>Sign Up</a>
    </button>
    {{/if}}
  </div>
</div>
</nav>